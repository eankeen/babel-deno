{
  "compilerOptions": {
    "outDir": "build",
    "sourceMap": true,
    "target": "ES2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
        "ESNext"
    ]
  }
}
