{
  "extends": ["airbnb-typescript/base"],
  "parserOptions": {
    "project": "./tsconfig.base.json"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-restricted-syntax": "off",
    "guard-for-in": "off",
    "no-unused-vars": "off",
    "no-unused-labels": "off",
    "@typescript-eslint/no-unused-vars": "off"
  },
  "overrides": [
    {
      "files": "**/*.test.ts",
      "env": {
        "jest": true
      }
    }
  ]
}
