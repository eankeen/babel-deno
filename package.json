{
  "name": "babel-deno",
  "private": true,
  "version": "0.0.0",
  "description": "Tools for transpiling Node APIs to their Deno counterparts",
  "scripts": {
    "clean": "lerna run clean",
    "test": "node --experimental-top-level-await test.mjs",
    "test:all": "lerna run test",
    "lint": "run-s lint:js lint:misc",
    "lint:fix": "run-s lint:js:fix lint:misc:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "lint:misc": "prettier --check .",
    "lint:misc:fix": "prettier --write ."
  },
  "engines": {
    "node": ">=14.3.0"
  },
  "author": "Edwin Kofler",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/types": "^7.9.6",
    "@types/babel__core": "^7.1.7",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.6",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "commitizen": "^4.1.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "lerna": "^3.20.2",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-toml": "^0.3.1",
    "prompts": "^2.3.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "cz-conventional-changelog": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
